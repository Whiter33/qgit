{
    "name": "QGit",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Quickly jump to git repos",
    "homepage_url": "http://johnlindquist.com",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "default_locale": "en",
    "background": {
        "page": "src/bg/background.html",
        "persistent": true
    },
    "options_page": "src/options/index.html",

    "omnibox": {
        "keyword": "qgit"
    },
    "permissions": [
        "http://cdnjs.com/"
    ]
}